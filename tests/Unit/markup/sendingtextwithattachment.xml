<main>
    <get_started>
        <bot>
            <message_type>
                <type>RESPONSE</type>
                <!--
                    <tag> is  for type MESSAGE_TAG only
                    @see https://developers.facebook.com/docs/messenger-platform/send-messages#standard_messaging
                -->
                <!--tag>ACCOUNT_UPDATE</tag-->
            </message_type>
            <message>
                <attachment>
                    <type>image</type>
                    <payload>
                        <url>http://www.messenger-rocks.com/image.jpg</url>
                        <is_reusable>true</is_reusable>
                    </payload>
                </attachment>
            </message>
        </bot>
    </get_started>
</main>